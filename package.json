{
  "name": "vue-ssr-start-cli",
  "description": "A vue.js with hot-reload dev and ssr.",
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "rimraf dist && cross-env NODE_ENV=production webpack --config build/webpack.client.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.js --progress --hide-modules",
    "eslint:fix": "./node_modules/.bin/eslint ./ --fix"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.4",
    "js-cookie": "^2.2.0",
    "lru-cache": "^5.1.1",
    "serve-favicon": "^2.5.0",
    "vue": "^2.5.21",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.21",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^2.0.4",
    "css-loader": "^2.0.2",
    "eslint": "^5.12.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-vue": "^5.1.0",
    "file-loader": "^3.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "px2rem-loader": "^0.1.9",
    "rimraf": "^2.6.2",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.1.2",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.21",
    "webfonts-loader": "^4.3.0",
    "webpack": "^4.28.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-merge": "^4.1.5",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
