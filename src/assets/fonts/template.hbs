@font-face {
font-family: "{{fontName}}";
src: {{{src}}};
font-weight: normal;
font-style: normal;
}

[class^="{{classPrefix}}"], [class*=" {{classPrefix}}"] {
font-family: {{fontName}} !important;
speak: none;
font-style: normal;
font-weight: normal;
font-variant: normal;
text-transform: none;
line-height: 1;
font-size: 20px;/*px*/
-webkit-font-smoothing: antialiased;
-moz-osx-font-smoothing: grayscale;
}

{{#each codepoints}}
    .{{../classPrefix}}{{@key}}:before {
    content: "\\{{this}}";
    }
{{/each}}